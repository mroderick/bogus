{
    "evil": true,
    "devel": false,
    "browser": true,

    "strict": true,
    "globalstrict": true,
    "forin": true,
    "immed": true,
    "newcap": true,
    "noarg": true,
    "curly": true,
    "trailing": true,
    "eqeqeq": true,
    "bitwise": true,
    "noempty": true,
    "latedef": true,
    "nonew": true,
    "maxdepth": 3,
    "undef": true,
    "unused": true,
    "maxparams": 20,
    "maxstatements": 20,
    "maxcomplexity": 10,
    "maxlen": 120,
    "quotmark": "single",
    "indent": 4,

    "predef": [
        "describe",
        "before",
        "beforeEach",
        "after",
        "afterEach",
        "it",
        "console",
        "define",
        "require"
    ]
}
